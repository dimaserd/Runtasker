@model string
@{
    ViewBag.Title = "Logs";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string log = ViewData["log"] as string;
    if (log == null) { log = "{не указано}"; }
}

<h2>Logs</h2>

<h4>Log = @log</h4>

<div>
    @if(Model != null)
    {
        @Model
    }
</div>

<h5>Log может быть равен {payments}, {notification}</h5>

